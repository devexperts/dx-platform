{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "declaration": true,
    "sourceMap": true,
    "target": "es5",
    "noEmitOnError": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "suppressImplicitAnyIndexErrors": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "pretty": true
  }
}
